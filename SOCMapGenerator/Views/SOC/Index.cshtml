@{
    ViewData["Title"] = "SOC Map Generator";
}
<div class="text-center">
    <h1 class="display-4">SOC Map Generator</h1>
    <p>Work in Progress</p>
    <section>
        <form>
            <h2>Creating a Map Template</h2>
            <p><label for="mapName">Map Name: </label> <input type="text" id="mapName" name="Map Name" /></p>
            <p><label for="description">Description: </label> <input type="text" id="description" name="Description" /></p>
            <p><label for="playerCount">Player Count: </label> <input type="number" id="playerCount" name="PlayerCount" /></p>
        </form>
    </section>
</div>